{
    "env": {
        "es2021": true,
        "node": true
    },
    // "extends": "standard-with-typescript",
    "extends": "standard",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "no-useless-constructor": "off",
        "camelcase": "off",
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-use-before-define": "off",
    }
}
